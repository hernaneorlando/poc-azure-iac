{
  "version": "2.0",
  "extensions": {
        "http": {
            "routePrefix": "function"
        }
    },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request, Exception",
        "maxTelemetryItemsPerSecond": 20
      },
      "enableLiveMetricsFilters": true
    }
  }
}
